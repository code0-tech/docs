{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":assignee(Taucher2003)",
    ":label(dependencies)",
    ":semanticCommitsDisabled"
  ],
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "@astrojs",
        "astro"
      ],
      "extends": [
        "schedule:monthly"
      ]
    }
  ]
}
